<div id="forms" class="clearfix">
	<h2>Forms</h2>
	<h3>Buttons</h3>
	<div>
		{{#each buttons}}
			<button class='inline {{className}}'>
				{{name}}
			</button>
		{{/each}}
		<div class="inline close"></div>
	</div>
	<h3>Dropdown</h3>
	<div class="dropdown">
		<a class="dropdown-toggle" data-toggle="dropdown" href="#">
			<span>Dropdown Name</span>
			<b class="caret"></b>
		</a>
		<ul class="dropdown-menu">
			<li>
				<h4>Menu Group</h4>
				<ul>
					<li class="label2a">
						Menu 1
					</li>
					<li class="label2a">
						Menu 2
					</li>

				</ul>
			</li>
		</ul>

	</div>
</div>
